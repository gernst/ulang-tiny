notation
  :  [infix 6];
  == != [infix 6];
  not [prefix 5];
  /\  [infixr 4];
  \/  [infixr 3];
  ==> [infixr 2];
  <=> [infix  1];

define
  o x   == o x;
  q x   == q x;
  h x   == h x;
  f x y == f x y;
  g x y == g x y;
  k x y == k x y;

inductive
  q (f x y) /\ p (k x y) ==> p (g x z);

assume
  p (h a);
show
  o a;
proof induction p _;