eval
  let f = (lambda x -> (F x)),
      g = (lambda x -> (G x)),
      h = (lambda x -> (H x))
  in
    f g h BASE
    AND
    ((f g) h) BASE
    AND
    f (g (h BASE))
    ;

  let f = (lambda x y z -> F (x y z)),
      g = (lambda x y -> G (x y)),
      h = (lambda x -> (H x))
  in
    f g h BASE;
