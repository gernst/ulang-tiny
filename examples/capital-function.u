data +;

define
  // function with lower case name
  f X == Y;
  // function with upper case name
  F X == Y;
  // function with data as name
  + X == Y;
  // function with symbol as name
  ++ X == Y;
  // function with number as name
  42 X == Y;
  // capital unicode letter
  Λ X == Y;
  // lower case unicode letter
  λ X == Y;

eval
  f X;
  F X;
  + X;
  ++ X;
  42 X;
  Λ X;
  λ X;
